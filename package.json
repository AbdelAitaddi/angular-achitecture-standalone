{
  "name": "ngx-apartment-search-standalone",
  "description": "This project represents a migration from Angular-architecture-modular to a Component-First architecture(standalone).",
  "version": "1.0.0",
  "author": "AbdelAitaddi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AbdelAitaddi/angular-achitecture-standalone/issues"
  },
  "homepage": "https://github.com/AbdelAitaddi/angular-achitecture-standalone/issues",
  "keywords": [
    "Angular-16",
    "component-first",
    "State-management",
    "standalone"
  ],
  "private": false,
  "engines": {
    "node": "^16.18.0"
  },
  "scripts": {
    "ng": "ng",
    "lint": "ng lint --fix",
    "start": "ng serve --force-esbuild",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "db": "json-server --watch db.json --delay 1000",
    "start-server": "angular-http-server --cors --path ./dist -p 4200",
    "format:fix": "pretty-quick",
    "prepare": "husky install",
    "test": "ng test"
  },
  "dependencies": {
    "@angular/animations": "~16.2.0",
    "@angular/cdk": "~16.2.5",
    "@angular/common": "~16.2.0",
    "@angular/compiler": "~16.2.0",
