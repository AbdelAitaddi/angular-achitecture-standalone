    "eslint-plugin-react": "~7.33.2",
    "husky": "8.0.3",
    "ng-mocks": "~14.11.0",
    "prettier": "~2.8.7",
    "pretty-quick": "~3.1.3",
    "ts-node": "~10.9.1",
    "typescript": "~5.1.3"
  }
